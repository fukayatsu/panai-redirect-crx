{
   "manifest_version": 2,
   "name": "PanaiRedirect",
   "version": "0.0.1",
   "options_page": "options.html",
   "permissions": [
      "webNavigation",
      "tabs"
   ],
   "background": {
      "scripts": [
         "js/background.js"
      ]
   },
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_title": "PanaiRedirect"
   },
   "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png"
   }
}
